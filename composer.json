        "hwi/oauth-bundle": "~0.5",
        "genemu/form-bundle": "^3.0@dev",
        "oneup/flysystem-bundle": "1.7.*",
        "intervention/image": "^2.3",
        "apy/datagrid-bundle": "^2.2",
        "stephpy/timeline-bundle": "~2.0",
